  },
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^3.10.0",
    "parallelshell": "^3.0.2",
    "postcss": "^6.0.16",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0"
  },
  "dependencies": {}
}
